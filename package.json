{
	"name": "vscode-matplotlib",
	"displayName": "vscode-matplotlib",
	"description": "something",
	"version": "0.0.2",
	"license": "MIT",
	"repository": "https://github.com/uktrade/vscode-matplotlib",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:mplPlotPanel.open",
		"onCommand:mplPlotPanel.clear",
		"onCommand:mplPlotPanel.runActiveDocument"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "mplPlotPanel.open",
				"title": "Matplotlib: Open plot panel"
			},
			{
				"command": "mplPlotPanel.clear",
				"title": "Matplotlib: Clear plot panel"
			},
			{
				"command": "mplPlotPanel.runActiveDocument",
				"title": "Matplotlib: Run active document and render plots in panel."
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "^14.18.11",
		"@types/vscode": "^1.64.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^9.2.0",
		"typescript": "^4.5.5",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {}
}
